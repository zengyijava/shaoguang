	<description>微站管理列表</description>
	<display-name>微站管理列表</display-name>
	<servlet-name>siteManagerSvt</servlet-name>
	<servlet-class>com.montnets.emp.wzgl.site.svt.wzgl_siteManagerSvt</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>siteManagerSvt</servlet-name>
	<url-pattern>/wzgl_siteManager.htm</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>siteManagerSvt</servlet-name>
	<url-pattern>/wzgl_siteManager.hts</url-pattern>
</servlet-mapping>
