SELECT A.PHONE, IFNULL(B.TIMES, 0) TIMES, B.VSTTM, B.SRCADDRESS, A.TASKID, A.TITLE, A.SENDTIME
FROM gw_mt_task_bak A
LEFT JOIN (SELECT TIMES,VSTTM,SRCADDRESS,PHONE FROM vst_detail202001 detail INNER JOIN (SELECT MAX(ID) TEMPID FROM vst_detail202001 GROUP BY PHONE, CUSTID) tempDetail ON detail.ID = tempDetail.TEMPID) B
ON B.CUSTID = A.TASKID AND A.PHONE = B.PHONE
INNER JOIN (SELECT TASKID FROM LF_RMS_URLTASK A LEFT JOIN lf_shorturl_param B ON A.SP_TEMPLID = B.SP_TEMPLID WHERE B.LONG_URL = 'http://coder520.com/') C 
ON A.TASKID = C.TASKID
WHERE A.ECID = '100001'